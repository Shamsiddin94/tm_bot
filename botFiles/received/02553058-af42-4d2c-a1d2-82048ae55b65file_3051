a:hover.script_link { font-family: Arial, Helvetica, sans-serif; font-size: 15px; color: rgb(255, 102, 51); font-weight: bold; text-decoration: underline; }

.delay_down { color: rgb(102, 102, 102); font-weight: bold; font-size: 10px; font-family: Arial, Helvetica, sans-serif; }

