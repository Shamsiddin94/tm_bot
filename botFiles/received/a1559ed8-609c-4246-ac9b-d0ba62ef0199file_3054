@charset "windows-1251";

@media print {
  body#list-print { background: none; color: rgb(0, 0, 0); margin: 1.5em 0.5em 1.5em 3.5em; }
  body#list-print p { padding: 20px 5px 5px 13px; }
  body#list-print thead th { background: none; border-bottom: 3px solid rgb(0, 0, 0); color: rgb(0, 0, 0); }
  body#list-print tfoot td { padding: 5px; background: none; border-top: 3px solid rgb(0, 0, 0); color: rgb(0, 0, 0); }
  body#list-print tbody td { background: none; border-bottom: 1px solid rgb(204, 204, 204); padding: 5px; }
  body#list-print table a:link, body#list-print table a:visited, body#list-print table a:hover, body#list-print table a:active { color: rgb(0, 0, 0); text-decoration: none; }
}
