a:hover.book_link { font-family: Verdana, Arial, Helvetica, sans-serif; font-size: 11px; color: rgb(0, 0, 0); text-decoration: underline; }

.delay { font-family: Arial, Helvetica, sans-serif; font-size: 15px; color: rgb(255, 102, 51); font-weight: bold; text-align: center; }

.sec { font-family: Arial, Helvetica, sans-serif; font-size: 21px; color: rgb(255, 102, 51); font-weight: bold; }

